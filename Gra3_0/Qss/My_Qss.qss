/*按钮静止无操作样式*/
.QPushButton
{
    background-color:rgb(255,255,255);
    color:rgb(6,168,255);
    border:3px solid rgb(6,168,255);
    font-size:25px;
    border-radius:10px;
}
/*鼠标悬停在按钮*/
.QPushButton:hover
{
    background-color:#a4c2f4;
    color:rgb(6,168,255);
    border:3px solid #d62929;
    border-radius:10px;
}
/*鼠标按下按钮*/
.QPushButton:pressed
{
    background-color:#4f917e;
    color:white;
    border:2px solid rgb(6,168,255);
    border-radius:10px;
}
